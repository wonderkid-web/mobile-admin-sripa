{
  "expo": {
    "name": "Admin Sripa",
    "slug": "expo-admin-sripa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon":"./assets/images/SRIPA.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/SRIPA.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mwahyuap.expoadminsripa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SRIPA.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mwahyuap.expoadminsripa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SRIPA.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e6b4f86e-bd34-461b-a06c-e99d934dfe0c"
      }
    }
  }
}
